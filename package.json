{
  "name": "pegasus-flutter",
  "scripts": {
    "prepublish": "cd widget_book && flutter build web --release -t lib/main.dart",
    "publish": "gh-pages -d ./widget_book/build/web",
    "figma:auth": "npx figma connect auth",
    "figma:parse": "npx figma connect parse --config .figma/figma.config.json",
    "figma:publish": "npx figma connect publish --config .figma/figma.config.json"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  },
  "dependencies": {
    "@figma/code-connect": "^1.3.13"
  }
}
